  "contracts": {
    "Router402": {
      "path": "contracts/Router402.sol",
      "description": "Main routing contract with fee collection and bridge integration",
      "functions": [
        "swap()",
        "swapViaSocket()",
        "calculateFee()",
        "pause()",
        "setEmergencyPause()",
        "emergencyWithdraw()"
      ]
    },
    "FeeCollector402": {
      "path": "contracts/FeeCollector402.sol",
      "description": "Simple fee collector for off-chain routing",
      "functions": [
        "payFee()",
        "payCustomFee()"
      ]
    },
    "Treasury402": {
      "path": "contracts/Treasury402.sol",
      "description": "2/3 multisig treasury for fee management",
      "functions": [
        "approveWithdrawal()"
      ]
    }
