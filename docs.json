  "contracts": {
    "Router402": {
      "path": "contracts/Router402.sol",
      "description": "Main routing contract with fee collection and bridge integration",
      "functions": [
        "swap()",
        "swapViaSocket()",
        "calculateFee()",
        "pause()",
        "setEmergencyPause()",
        "emergencyWithdraw()"
      ]
    },
    "FeeCollector402": {
      "path": "contracts/FeeCollector402.sol",
      "description": "Simple fee collector for off-chain routing",
      "functions": [
        "payFee()",
        "payCustomFee()"
      ]
    },
    "CREATE2Factory": {
      "path": "contracts/CREATE2Factory.sol",
      "description": "Deterministic contract deployment factory using CREATE2",
      "functions": [
        "deploy()",
        "deploySafe()",
        "computeAddress()",
        "hasCode()"
      ]
    }
